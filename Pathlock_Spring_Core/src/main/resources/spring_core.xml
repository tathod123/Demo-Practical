<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>

      <!--  <bean id="s1" class="com.spring.Student" scope="prototype" Autowire="byName">-->
      <bean id="s1" class="com.spring.Student" scope="prototype" >
      <property name="sid" value="111"/>
      <property name="sname" value="shubham"/>
      <property name="percent" value="85"/>
      <property name="course" ref="c1"/>
      </bean>
      
      <bean id="s2" class="com.spring.Student" scope="prototype">
      <property name="sid" value="112"/>
      <property name="sname" value="kavi"/>
      <property name="percent" value="80"/>
      <property name="course" ref="c1"/>
      
      </bean>
      
     <bean id="s3" class="com.spring.Student" scope="prototype">
      <constructor-arg value="101" />
      <constructor-arg value="Neha" />
      <constructor-arg value="75" />
   <!--   <property name="course" ref="c1"/>-->
      </bean>
      
       <bean id="c1" class="com.spring.course">
      <constructor-arg  value="java" />
      <constructor-arg  value="500000" />
      
      </bean>
        <bean id="c2" class="com.spring.course">
      <constructor-arg  value="python" />
      <constructor-arg  value="400000" />
      
      </bean>
      
        
      
</beans>